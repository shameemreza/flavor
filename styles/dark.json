{
	"$schema": "https://schemas.wp.org/wp/6.7/theme.json",
	"version": 3,
	"title": "Dark",
	"settings": {
		"color": {
			"palette": [
				{
					"slug": "base",
					"color": "#111827",
					"name": "Base"
				},
				{
					"slug": "surface",
					"color": "#1F2937",
					"name": "Surface"
				},
				{
					"slug": "contrast",
					"color": "#F9FAFB",
					"name": "Contrast"
				},
				{
					"slug": "primary",
					"color": "#94A3B8",
					"name": "Primary"
				},
				{
					"slug": "secondary",
					"color": "#64748B",
					"name": "Secondary"
				},
				{
					"slug": "muted",
					"color": "#9CA3AF",
					"name": "Muted"
				},
				{
					"slug": "subtle",
					"color": "#374151",
					"name": "Subtle"
				},
				{
					"slug": "accent",
					"color": "#CBD5E1",
					"name": "Accent"
				}
			]
		}
	},
	"styles": {
		"color": {
			"background": "var(--wp--preset--color--base)",
			"text": "var(--wp--preset--color--contrast)"
		},
		"elements": {
			"button": {
				"color": {
					"background": "var(--wp--preset--color--primary)",
					"text": "var(--wp--preset--color--base)"
				},
				":hover": {
					"color": {
						"background": "var(--wp--preset--color--accent)"
					}
				}
			},
			"link": {
				"color": {
					"text": "var(--wp--preset--color--primary)"
				},
				":hover": {
					"color": {
						"text": "var(--wp--preset--color--accent)"
					}
				}
			}
		},
		"blocks": {
			"core/code": {
				"color": {
					"background": "var(--wp--preset--color--surface)",
					"text": "var(--wp--preset--color--contrast)"
				}
			},
			"core/preformatted": {
				"color": {
					"background": "var(--wp--preset--color--surface)",
					"text": "var(--wp--preset--color--contrast)"
				}
			}
		}
	}
}

